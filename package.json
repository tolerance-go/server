{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-server",
    "stop": "egg-scripts stop --title=egg-server-server",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts --resolve-plugins-relative-to .",
    "clean": "ets clean",
    "init:db": "npx sequelize migration:generate --name=discuss-update",
    "migrate": "NODE_ENV=development npx sequelize db:migrate",
    "migrate:undo": "NODE_ENV=development npx sequelize db:migrate:undo",
    "migrate:test": "NODE_ENV=test npx sequelize db:migrate",
    "migrate:production": "NODE_ENV=production npx sequelize db:migrate"
  },
  "dependencies": {
    "egg": "^2.6.1",
    "egg-passport": "^2.1.1",
    "egg-passport-github": "^1.0.0",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "fs-extra": "^10.1.0",
    "invariant": "^2.2.4",
    "koa-static-cache": "^5.1.4",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "node-rsa": "^1.1.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/node-rsa": "^1.1.1",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^2.1.0",
    "egg-mock": "^3.16.0",
    "egg-swagger-doc-custom": "^2.3.3",
    "eslint": "^8.0.0",
    "eslint-config-egg": "^12.0.0",
    "prettier": "^2.7.1",
    "sequelize-cli": "^6.4.1",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "ci": {
    "version": "14, 16, 18"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "",
  "license": "MIT"
}
